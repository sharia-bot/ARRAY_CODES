class Solution {
public:
    void duplicateZeros(vector<int>& arr) {
         int n = arr.size();
         queue<int>q;

         for(int i=0;i<n;i++){
            if(arr[i] == 0){
                q.push(0);
                q.push(0);
            }
            else
               q.push(arr[i]);

            arr[i] = q.front();
            q.pop();   
         }
    return;    
    }
};
