class Solution {
public:
    int heightChecker(vector<int>& heights) {
        vector<int>temp = heights;

        sort(begin(temp),end(temp));
        
        int n = temp.size();

        int totalMissMatch  = 0;

        for(int i=0;i<n;i++){
            if(temp[i] != heights[i]){
                totalMissMatch++;
            }
        }
    return totalMissMatch;

        
    }
};
