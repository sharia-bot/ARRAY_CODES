class Solution {
public:
    vector<int> sumZero(int n) {
        vector<int>result;

        int half_n = n/2;

        for(int i=1;i<=half_n;i++){
            result.push_back(i);
            result.push_back(-i);
        }

        if(n%2 == 1)
           result.push_back(0);

    return result;       
        
    }
};
