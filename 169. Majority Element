Approach-1 using sorting 
T.C - O(NLOGN)
S.C- O(1)
class Solution {
public:
    int majorityElement(vector<int>& nums) {
        if(nums.size() == 1) return nums[0];
        
        sort(begin(nums),end(nums));
        int n = nums.size();
        int half_n = n/2;
        
        int count = 1;

        for(int i=0;i<n-1;i++){
            if(nums[i] == nums[i+1]){
                count++;
                if(count>half_n)
                   return nums[i];
            }
            else
             count = 1;
        }             
    return 0;    
        
    }
};
Approach -2 
Using Booye Moyer Majority Vote Algorithtm
T.C - O(N)
S.C- O(1)
class Solution {
public:
    int majorityElement(vector<int>& nums) {
        int count    = 0;
        int candidate= 0;

        for(int& num : nums){
            if(count == 0){
                candidate = num;
                count++;
            }
            else if(candidate == num){
                count++;
            }
            else
              count--;
        } 
    return candidate;    
    }
};
Using Bit Manipulation
T.C - O(N)
class Solution {
public:
    int majorityElement(vector<int>& nums) {

        int n = nums.size();
        int half_n = n/2;
        int candidate = 0;

        for(int i =0;i<32;i++){
            int One_bit_count = 0;

             for(auto & num : nums){
                int ith_bit = (num >> i) & 1;

                if(ith_bit == 1)
                  One_bit_count++;
             }
             if(One_bit_count>half_n){
               candidate |= (1<<i);
             }
        }
    return candidate;    
    }
};
